{
  "name": "tiaoom",
  "version": "0.0.48",
  "description": "",
  "main": "./src/index.cjs",
  "module": "./src/index.mjs",
  "types": "./src/index.d.cts",
  "typesVersions": {
    "*": {
      "client": [
        "./src/client.d.ts"
      ]
    }
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsdown",
    "docs:build": "cd docs && npm run docs:build",
    "docs:dev": "cd docs && npm run docs:dev"
  },
  "files": [
    "src",
    "README.md",
    "logo.png"
  ],
  "keywords": [
    "game",
    "framework",
    "multiplayer"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "cross-env": "^10.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsdown": "^0.17.0-beta.5",
    "typescript": "^5.0.4"
  },
  "unpkg": "./src/client.iife.js",
  "jsdelivr": "./src/client.iife.js",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.mjs",
      "require": "./src/index.cjs"
    },
    "./client": {
      "types": "./src/client.d.ts",
      "import": "./src/client.mjs",
      "require": "./src/client.cjs",
      "default": "./src/client.iife.js"
    }
  }
}
